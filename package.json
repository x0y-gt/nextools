{
  "name": "nextools",
  "version": "0.9.0",
  "description": "React toolkit library focused on nextjs, RSC, tailwind and shadcn",
  "author": "x0y <x0y@milkyweb.co>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/nextools/index.cjs",
  "module": "./dist/nextools/index.js",
  "types": "./dist/nextools/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nextools/index.js",
      "require": "./dist/nextools/index.cjs",
      "types": "./dist/nextools/index.d.ts"
    },
    "./types": "./dist/nextools/index.d.ts",
    "./forms": {
      "import": "./dist/nextools/forms.js",
      "require": "./dist/nextools/forms.cjs",
      "types": "./dist/nextools/forms.d.ts"
    },
    "./utils": {
      "import": "./dist/nextools/utils.js",
      "require": "./dist/nextools/utils.cjs",
      "types": "./dist/nextools/utils.d.ts"
    },
    "./location": {
      "import": "./dist/nextools/location.js",
      "require": "./dist/nextools/location.cjs",
      "types": "./dist/nextools/location.d.ts"
    },
    "./recaptcha": {
      "import": "./dist/nextools/recaptcha.js",
      "require": "./dist/nextools/recaptcha.cjs",
      "types": "./dist/nextools/recaptcha.d.ts"
    },
    "./files": {
      "import": "./dist/nextools/files.js",
      "require": "./dist/nextools/files.cjs",
      "types": "./dist/nextools/files.d.ts"
    },
    "./files/service": {
      "import": "./dist/nextools/filesService.js",
      "require": "./dist/nextools/filesService.cjs",
      "types": "./dist/nextools/filesService.d.ts"
    },
    "./formatters": {
      "import": "./dist/nextools/formatters.js",
      "require": "./dist/nextools/formatters.cjs",
      "types": "./dist/nextools/formatters.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "aws-sdk": "^2.1692.0",
    "iron-session": "^8.0.3",
    "leaflet": "^1.9.4",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.52.2",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0-rc.2",
    "rollup": "^4.21.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.0.0",
    "@hookform/resolvers": "^5.0.0",
    "zod": "^3.0.0",
    "react-icons": "^5.0.0"
  }
}
